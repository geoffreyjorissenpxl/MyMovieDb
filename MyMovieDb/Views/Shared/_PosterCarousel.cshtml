@using MyMovieDb.Domain
@model IList<MediaImage>

<button class="previous-btn"><img src="images/previous.png" alt=""></button>
<button class="next-btn"><img src="images/next.png" alt=""></button>
<div class="card-container">
    @foreach (var movie in Model)
    {
        <div class="poster-card">
            <img src=@("https://image.tmdb.org/t/p/w400/" + movie.ImageLink) class="poster-thumbnail" />
        </div>


    }
</div>